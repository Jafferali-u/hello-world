pipeline{ 
  agent any   
    environment{
        PATH = "/opt/maven3/bin:$PATH"
    }
  stages{
        stage("mevan build"){
            steps {
                sh "mvn clean package"
            }
        }
        stage("code check"){
            steps{
                withSonarQubeEnv(credentialsId: 'sonar') {
                    sh "mvn sonar:sonar"
                }
             }
        }
    }
}
